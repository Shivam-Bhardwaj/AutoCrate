# AutoCrate Environment Variables Example
# Copy this file to .env.local and configure as needed

# =============================================================================
# DEVELOPMENT SETTINGS
# =============================================================================

# Next.js Environment
# NODE_ENV=development

# Development server port (optional - Next.js will auto-select if not specified)
# PORT=3000

# =============================================================================
# API CONFIGURATION
# =============================================================================

# External API endpoints for future integrations
# API_BASE_URL=https://api.example.com
# NX_CAD_API_URL=https://nx-api.example.com

# API keys for external services
# MATERIALS_API_KEY=your_materials_api_key_here
# SHIPPING_API_KEY=your_shipping_api_key_here
# PRICING_API_KEY=your_pricing_api_key_here

# =============================================================================
# ANALYTICS & MONITORING
# =============================================================================

# Google Analytics tracking ID
# NEXT_PUBLIC_GA_TRACKING_ID=GA-XXXXXXXXX

# Application monitoring
# SENTRY_DSN=https://your-sentry-dsn-here
# NEXT_PUBLIC_SENTRY_DSN=https://your-public-sentry-dsn-here

# Performance monitoring
# VERCEL_ANALYTICS_ID=your_vercel_analytics_id

# =============================================================================
# FEATURE FLAGS
# =============================================================================

# Enable/disable features during development
# NEXT_PUBLIC_ENABLE_BETA_FEATURES=false
# NEXT_PUBLIC_ENABLE_ADVANCED_MATERIALS=false
# NEXT_PUBLIC_ENABLE_COST_CALCULATOR=true
# NEXT_PUBLIC_ENABLE_3D_EXPORT=false

# =============================================================================
# DATABASE & STORAGE
# =============================================================================

# Database connection (if implementing user accounts)
# DATABASE_URL=postgresql://user:password@localhost:5432/autocrate
# REDIS_URL=redis://localhost:6379

# File storage for future 3D model exports
# AWS_ACCESS_KEY_ID=your_aws_access_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret_key
# AWS_REGION=us-east-1
# AWS_S3_BUCKET=autocrate-exports

# =============================================================================
# EMAIL & NOTIFICATIONS
# =============================================================================

# Email service for sharing designs or notifications
# EMAIL_SERVICE_API_KEY=your_email_service_key
# FROM_EMAIL=noreply@autocrate.com

# =============================================================================
# SECURITY
# =============================================================================

# Authentication (if implementing user accounts)
# NEXTAUTH_SECRET=your_nextauth_secret_here
# NEXTAUTH_URL=http://localhost:3000

# JWT secret for API authentication
# JWT_SECRET=your_jwt_secret_here

# =============================================================================
# DEPLOYMENT SPECIFIC
# =============================================================================

# Vercel deployment settings
# VERCEL_TOKEN=your_vercel_token
# VERCEL_ORG_ID=your_org_id
# VERCEL_PROJECT_ID=your_project_id

# CDN settings for assets
# NEXT_PUBLIC_CDN_URL=https://cdn.autocrate.com

# =============================================================================
# LOGGING & DEBUG
# =============================================================================

# Enable debug logging
# DEBUG=autocrate:*
# LOG_LEVEL=info

# External logging service
# LOG_SERVICE_API_KEY=your_logging_service_key