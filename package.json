{
  "name": "autocrate",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:logged": "node scripts/logged-build.js build",
    "dev:logged": "node scripts/logged-build.js dev",
    "clean": "rimraf .next out",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "test": "vitest",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "e2e": "node tests/e2e/run-e2e.js --prod",
    "e2e:prod": "node tests/e2e/run-e2e.js --prod",
    "e2e:pw": "playwright test",
    "e2e:pw:headed": "playwright test --headed",
    "e2e:pw:ui": "playwright test --ui",
    "e2e:headed": "node tests/e2e/run-e2e.js --headed",
    "e2e:debug": "node tests/e2e/run-e2e.js --debug",
    "e2e:no-server": "node tests/e2e/run-e2e.js --no-server",
    "test:all": "npm run test:unit && npm run test:integration",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "email:generate": "node scripts/generate-email.js",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "deploy:build": "npm run build && vercel --prod --yes",
    "auto:develop": "node scripts/auto-develop.js",
    "auto:deploy": "npm run build && npm run deploy",
    "auto:fix": "npm run format && npm run lint -- --fix",
    "auto:pr": "gh pr create --fill --base main",
    "auto:merge": "gh pr merge --auto --squash --delete-branch",
    "auto:complete": "npm run auto:fix && git add -A && git commit -m 'auto: apply changes' && git push && npm run auto:pr && npm run auto:merge && npm run auto:deploy",
    "monitor": "node scripts/monitor.js",
    "monitor:local": "node scripts/monitor.js local",
    "monitor:prod": "node scripts/monitor.js production",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@react-three/drei": "^9.88.11",
    "@react-three/fiber": "^8.15.11",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.294.0",
    "next": "^14.2.32",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^2.1.0",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.159.0",
    "zod": "^4.1.5",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.1",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@types/three": "^0.159.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.10.3",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.4",
    "glob": "^11.0.3",
    "happy-dom": "^18.0.1",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-junit": "^16.0.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^16.1.6",
    "msw": "^2.11.1",
    "pa11y": "^9.0.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "puppeteer": "^24.19.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^6.0.1",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vercel": "^47.0.5",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.4"
  }
}
